# R3 to R1
auto eth1
iface eth1 inet static
    address 10.0.0.3
    netmask 255.255.255.254

# R3 to R2
auto eth0
iface eth0 inet static
    address 10.0.0.9
    netmask 255.255.255.254

# R3 to R4
auto eth2
iface eth2 inet static
    address 10.0.0.10
    netmask 255.255.255.254

# Subnet Interface
auto eth3
iface eth3 inet static
    address 151.100.165.1
    netmask 255.255.255.0
